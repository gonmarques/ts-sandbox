{
  "name": "ts-exp",
  "version": "0.0.1",
  "description": "TypeScript experiment",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "webpack",
    "rebuild": "rimraf dist && webpack",
    "clean": "rimraf dist",
    "test": "mocha -r ts-node/register src/test/**/*.ts",
    "test:watch": "mocha -r ts-node/register src/test/**/*.ts --watch-extensions ts --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.0",
    "@types/deep-equal": "^1.0.0",
    "@types/mocha": "^2.2.5",
    "@types/node": "^4.0.27-alpha",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-immutable": "^1.6.0",
    "deep-equal": "^1.0.1",
    "immutable": "^3.8.1",
    "mocha": "^3.3.0",
    "rimraf": "^2.5.3",
    "sinon": "^2.2.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^3.0.2",
    "typescript": "^2.0.0",
    "webpack": "^1.13.1"
